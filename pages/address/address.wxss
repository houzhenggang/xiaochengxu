page{
  width:100%;
  height: 100%;
  background: #f7f7f7;
}
.container{
  width: 100%;
  box-sizing: border-box;
  padding:0 34rpx;  
  background: #fff;
  margin-top:20rpx;
  overflow: hidden;
}
.contentBtm image{
  width: 32rpx;
  height: 32rpx;
   vertical-align: middle;
   margin-right: 8rpx;
}
.contentBtm{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 64rpx;
  border-top:1rpx solid #ebebeb;
}
.contentBtm label{
   font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #999999;
    display: flex;
    align-items: center;
}
.contentBtm label:nth-child(2){
  margin-left:215rpx;
}
.userAddr , .userInfo{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #333333;
}
 .userInfo{
   margin-top:18rpx;
 }
 .userInfo label:nth-child(2){
   margin-left:30rpx;
 }
.userAddr{
  margin-top:14rpx;
  margin-bottom:20rpx;
}
.address{
  position: fixed;
  bottom:0;
  width:100%;
  left:0;
  background: #EA2534;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  font-family: PingFangSC-Regular;
  font-size: 36rpx;
  color: #FFFFFF;
}