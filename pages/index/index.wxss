/**index.wxss**/
.container_search_navi{
	padding-left: 58rpx;
	position: relative;
	background: #EDEDED;
	font-size: 22rpx;
	line-height: 56rpx;
	width: 652rpx;
	height: 56rpx;
	margin: 0 auto;
	border-radius: 56rpx;
	color: #999999;
	letter-spacing: 1rpx;
}
.container_search icon{
	display: block;
	width: 24rpx;
	height: 24rpx;
	position: absolute;
	top: 12rpx;
	left: 20rpx;
}

.index_banner1{
	display: block;
	width: 710rpx;
	height: 288rpx;
	margin: 0 auto;
	margin-top: 22rpx;
}

.index_classify{
	height: 128rpx;
	width: 710rpx;
	margin: 0 auto;
	margin-top: 32rpx;
	font-size: 24rpx;
	line-height: 34rpx;
	display: flex;
	justify-content: space-between;
	letter-spacing: 1rpx;
}
.index_classify view{
	display: flex;
	height: 128rpx;
	width: 710rpx;
	flex-direction: column;
	align-items: center;
	justify-content: space-between
}
.index_classify image{
	display: block;
	width: 80rpx;
	height: 80rpx;
}

.index_current{
	width: 82rpx;
	height: 12rpx;
	display: flex;
	margin: 0 auto;
	margin-top: 24rpx;
	text-align: center;
	justify-content: space-between;
}
.index_current view{
	width: 12rpx;
	height: 12rpx;
	background: #E9E9E9;
	border-radius: 12rpx;
}
.index_current view:first-child{
	width: 28rpx;
	background: #FF7C86;
}

.index_contact{
	height: 240rpx;
	width: 668rpx;
	margin: 0 auto;
	margin-top: 62rpx;
	position: relative;
	background: #EDEDED;
	overflow: hidden;
	letter-spacing: 1rpx;
	border-radius: 10rpx;
}
.index_contact .contact_h4{
	display: block;
	margin-top: 32rpx;
	font-family: PingFangSC-Light;
	font-size: 28rpx;
	color: #333333;
	line-height: 40rpx;
	padding-left: 48rpx;
}
.index_contact .contact_p{
	display: block;
	font-size: 26rpx;
	margin-top: 10rpx;
	width: 440rpx;
	height: 132rpx;
	margin-left: 46rpx;
	font-family: PingFangSC-Regular;
	color: #333333;
	line-height: 44rpx;
}
.index_contact button{
	width: 66rpx;
	height: 64rpx;
	padding: 0;
	position: absolute;
	top: 46rpx;
	right: 52rpx;
	border-radius: 64rpx 66rpx;
	background: #EDEDED;
}
.index_contact button image{
	width: 66rpx;
	height: 64rpx;
}
.index_contact button::after{
	border: none;
}
.index_contact image:nth-child(4){
	top: 130rpx;
	right: 52rpx;
	width: 66rpx;
	height: 64rpx;
	position: absolute;
}

.index_recommend{
	position: relative;
	margin-top: 78rpx;
	width: 750rpx;
	display: flex;
	align-items: center;
	height: 44rpx;
}
.index_recommend text{
	display: inline-block;
	font-size: 32rpx;
	line-height: 44rpx;
	font-weight: 600;
	font-family: PingFangSC-Semibold;
	color: #333333;
}
.index_recommend text:nth-child(1){
	margin-left: 290rpx;
}
.index_recommend text:nth-child(2){
	margin-left: 42rpx;
}
.index_recommend text:nth-child(3){
	font-size: 22rpx;
	font-family: PingFangSC-Regular;
	margin-left: 162rpx;
	color: #999999;
	line-height: 36rpx;
}
.index_recommend image:nth-child(4){
	position: absolute;
	width: 338rpx;
	height: 36rpx;
	left: 192rpx;
}
.index_recommend image:nth-child(5){
	width: 12rpx;
	height: 20rpx;
	position: absolute;
	right: 32rpx;
	bottom: 14rpx;
}

.index_re_produ{
	width: 710rpx;
	margin: 0 auto;
	margin-top: 48rpx;
	position: relative;
}
.re_produ_img{
	display: block;
	width: 710rpx;
	height: 340rpx;
	margin: 0 auto;
}
.re_produ_text{
	display: block;
	font-size: 26rpx;
	font-family: PingFangSC-Regular;
	margin-top: 16rpx;
	letter-spacing: 1rpx;
	color: #333333;
	line-height: 36rpx;
}
.re_text_view{
	display: flex;
	align-items: center;
	height: 32rpx;
	margin-top: 8rpx;
}
.re_text_view text:nth-child(1){
	font-size: 22rpx;
	font-family: PingFangSC-Regular;
	color: #EA2534;
	line-height: 32rpx;
}
.re_text_view text:nth-child(2){
	font-size: 32rpx;
	font-family: PingFangSC-Regular;
	color: #EA2534;
	line-height: 32rpx;
	margin-right: 18rpx;
}
.re_text_view text:nth-child(3){
	font-size: 22rpx;
	font-family: PingFangSC-Regular;
	color: #999999;
	line-height: 32rpx;
}
.index_re_produ .buy_button{
	position: absolute;
	bottom: 0;
	right: 0;
	height: 50rpx;
	width: 140rpx;
	font-size: 22rpx;
	line-height: 50rpx;
	color: #FFFFFF;
	text-align: center;
	background: #EA2534;
	border-radius: 50rpx;
}

.index_item{
	width: 710rpx;
	margin: 0 auto;
	margin-top: 24rpx;
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}
.item_box{
	width: 344rpx;
	height: 484rpx;
	margin-bottom: 22rpx;
}
.item_box image{
	width: 344rpx;
	height: 344rpx;
	display: block;
}
.item_box .item_text{
	display: block;
	font-size: 24rpx;
	font-family: PingFangSC-Regular;
	margin-top: 14rpx;
	color: #333333;
	line-height: 36rpx;
}
.item_box .item_text_view{
	display: flex;
	align-items: center;
	height: 32rpx;
	margin-top: 8rpx;
	position: relative;
}
.item_text_view text:nth-child(1){
	font-size: 22rpx;
	font-family: PingFangSC-Regular;
	color: #EA2534;
	line-height: 32rpx;
}
.item_text_view text:nth-child(2){
	font-size: 26rpx;
	font-family: PingFangSC-Regular;
	color: #EA2534;
	line-height: 32rpx;
	margin-right: 18rpx;
}
.item_text_view text:nth-child(3){
	font-size: 22rpx;
	font-family: PingFangSC-Regular;
	color: #999999;
	line-height: 32rpx;
	position: absolute;
	right: 0;
}

.index_special{
	margin-top: 46rpx;
}
.special_item text:nth-child(2){
	margin-right: 0;
}
.special_item text:nth-child(3){
	position: static;
	text-decoration:line-through;
}

.bottom{
	width: 750rpx;
	height: 80rpx;
	background: #F6F7F7;
	text-align: center;
	font-size: 22rpx;
	line-height: 80rpx;
	color: #999999;
	font-family: PingFangSC-Regular;
	position: absolute;
}
.bottom::before{
		content: "";
		border-top: 1px solid #DCDCDC;
		display: block;
		position: absolute;
		width: 88rpx;
		top: 39rpx;
		left: 156rpx;
}
.bottom::after{
		content: "";
		border-top: 1px solid #DCDCDC;
		display: block;
		position: absolute;
		width: 88rpx;
		top: 39rpx;
		right: 156rpx;
}